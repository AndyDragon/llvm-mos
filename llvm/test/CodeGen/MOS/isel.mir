    %7:any(s1) = G_CONSTANT i1 true
    %8:any(s1) = G_XOR %2, %7
    G_BRCOND %8, %bb.0
    RTS
# CHECK:      %8:cc = CMPimm %0, 42
# CHECK-NEXT: BR %bb.0, %8, 0
...
