# RUN: llc -mtriple=mos -run-pass=postrapseudos -verify-machineinstrs -o - %s | FileCheck %s
---
name:            tax
tracksRegLiveness: true
# CHECK-LABEL: name: tax
body:             |
  bb.0.entry:
    liveins: $a
    $x = COPY $a
# CHECK: $x = TA_
...
---
name:            txa
tracksRegLiveness: true
# CHECK-LABEL: name: txa
body:             |
  bb.0.entry:
    liveins: $x
    $a = COPY $x
# CHECK: T_A $x
...
---
name:            tay
tracksRegLiveness: true
# CHECK-LABEL: name: tay
body:             |
  bb.0.entry:
    liveins: $a
    $y = COPY $a
# CHECK: $y = TA_
...
---
name:            tya
tracksRegLiveness: true
# CHECK-LABEL: name: tya
body:             |
  bb.0.entry:
    liveins: $y
    $a = COPY $y
# CHECK: T_A $y
...
---
name:            txy
tracksRegLiveness: true
# CHECK-LABEL: name: txy
body:             |
  bb.0.entry:
    liveins: $x
    $y = COPY $x
# CHECK:      T_A $x
# CHECK-NEXT: $y = TA_
...
---
name:            tyx
tracksRegLiveness: true
# CHECK-LABEL: name: tyx
body:             |
  bb.0.entry:
    liveins: $y
    $x = COPY $y
# CHECK:      T_A $y
# CHECK-NEXT: $x = TA_
...
---
name:            preserve_a_txy
tracksRegLiveness: true
# CHECK-LABEL: name: preserve_a_txy
body:             |
  bb.0.entry:
    liveins: $a, $x
    $y = COPY $x
    CMPimm $a, 0, implicit-def $c, implicit-def $nz
# CHECK:      PHA
# CHECK-NEXT: T_A $x
# CHECK-NEXT: $y = TA_
# CHECK-NEXT: PLA
...
---
name:            stzp
tracksRegLiveness: true
# CHECK-LABEL: name: stzp
body:             |
  bb.0.entry:
    liveins: $a
    $rc0 = COPY $a
# CHECK: $rc0 = STimag8 $a
...
---
name:            ldzp
tracksRegLiveness: true
# CHECK-LABEL: name: ldzp
body:             |
  bb.0.entry:
    liveins: $rc0
    $a = COPY $rc0
# CHECK: $a = LDimag8 $rc0
...
---
name:            zptozp_a
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_a
body:             |
  bb.0.entry:
    liveins: $rc0
    $rc1 = COPY $rc0
# CHECK:      $a = LDimag8 $rc0
# CHECK-NEXT: $rc1 = STimag8 $a
...
---
name:            zptozp_save
tracksRegLiveness: true
# CHECK-LABEL: name: zptozp_save
body:             |
  bb.0.entry:
    liveins: $rc0, $a, $x, $y
    $rc1 = COPY $rc0
    RTS implicit $a, implicit $x, implicit $y
# CHECK:      PHA
# CHECK-NEXT: $a = LDimag8 $rc0
# CHECK-NEXT: $rc1 = STimag8 $a
# CHECK-NEXT: PLA
# CHECK-NEXT: RTS
...
---
name:            zpptr_to_zpptr
tracksRegLiveness: true
# CHECK-LABEL: name: zpptr_to_zpptr
body:             |
  bb.0.entry:
    liveins: $rs0
    $rs1 = COPY $rs0
# CHECK:      $a = LDimag8 $rc0
# CHECK-NEXT: $rc2 = STimag8 $a
# CHECK:      $a = LDimag8 $rc1
# CHECK-NEXT: $rc3 = STimag8 $a
...
