# RUN: llc -mtriple=mos -run-pass=greedy -verify-machineinstrs -o - %s | FileCheck %s
---
name: remat_ldimm
# CHECK-LABEL: name: remat_ldimm
legalized: true
regBankSelected: true
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $a
    %0:gpr = LDimm 42, implicit-def $nz
    JSR &fn, mos_csr
    $a = COPY %0
    RTS implicit $a
# CHECK: JSR
# CHECK-NEXT: [[REG:%[0-9]+]]:gpr = LDimm 42
# CHECK-NEXT: $a = COPY [[REG]]
# CHECK-NEXT: RTS
...
---
name: spill_y
# CHECK-LABEL: name: spill_y
legalized: true
regBankSelected: true
tracksRegLiveness: true
body: |
  bb.0.entry:
    liveins: $y
    %0:yc = COPY $y
    JSR &fn, mos_csr
    $y = COPY %0
    RTS implicit $y
# CHECK:      STstk $y, %stack.0, 0, implicit-def $y, implicit-def $nz :: (store 1 into %stack.0)
# CHECK-NEXT: JSR
# CHECK-NEXT: $y = LDstk %stack.0, 0, implicit-def $y, implicit-def $nz :: (load 1 from %stack.0)
...
