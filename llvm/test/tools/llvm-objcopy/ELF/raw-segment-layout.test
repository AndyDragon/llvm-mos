# RUN: yaml2obj %s -o %t
# RUN: llvm-objcopy -O raw %t %t2
# RUN: od -t x2 %t2 | FileCheck %s --ignore-case
# RUN: wc -c < %t2 | FileCheck %s --check-prefix=SIZE

!ELF
FileHeader:
  Class:           ELFCLASS64
  Data:            ELFDATA2LSB
  Type:            ET_EXEC
  Machine:         EM_X86_64
Sections:
  - Name:            .text
    Type:            SHT_PROGBITS
    Flags:           [ SHF_ALLOC, SHF_EXECINSTR ]
    AddressAlign:    0x0
    Address:         0x08
    Content:         "c3c3c3c3"
  - Name:            .header
    Type:            SHT_PROGBITS
    Flags:           [ ]
    AddressAlign:    0x0
    Address:         0x00
    Content:         "4242"
  - Name:            .data
    Type:            SHT_PROGBITS
    Flags:           [ SHF_ALLOC, SHF_EXECINSTR ]
    AddressAlign:    0x00
    Address:         0x00
    Content:         "3232"
ProgramHeaders:
  - Type:     PT_LOAD
    Flags:    [ PF_X, PF_R ]
    VAddr:    0x08
    FirstSec: .text
    LastSec:  .text
  - Type:     PT_LOAD
    Flags:    [ PF_R ]
    VAddr:    0x00
    FirstSec: .data
    LastSec:  .data

# CHECK:       c3c3 c3c3 4242 3232
# SIZE:        8
